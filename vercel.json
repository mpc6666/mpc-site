{
  "build": {
    "env": {
      "HUGO_VERSION": "0.140.0"
    }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/en/:path*",
      "destination": "/en/:path*"
    },
    {
      "source": "/de/:path*",
      "destination": "/de/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "mpccarbide.com"
        }
      ],
      "destination": "/en/",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "de.mpccarbide.com"
        }
      ],
      "destination": "/de/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "mpccarbide.com"
        }
      ],
      "destination": "/en/:path*",
      "permanent": false
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "de.mpccarbide.com"
        }
      ],
      "destination": "/de/:path*",
      "permanent": false
    }
  ]
}