{
  "build": {
    "env": {
      "HUGO_VERSION": "0.140.0"
    }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "mpccarbide.com"
        }
      ],
      "destination": "/en/",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "de.mpccarbide.com"
        }
      ],
      "destination": "/de/",
      "permanent": true
    }
  ]
}